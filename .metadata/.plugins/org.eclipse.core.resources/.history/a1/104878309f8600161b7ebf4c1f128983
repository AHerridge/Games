package jrAlex.core.math.vector;

/**
 * Created on 9/13/2016.
 */

public class Vector3D extends Vector
{
	public Vector3D(double x, double y, double z)
	{
		super(new double[]{x, y, z});
	}

	public double getX()
	{
		return getValue(0);
	}

	public double getY()
	{
		return getValue(1);
	}

	public double getZ()
	{
		return getValue(2);
	}
}
