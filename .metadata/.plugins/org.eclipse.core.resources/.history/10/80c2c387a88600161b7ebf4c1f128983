package jrAlex.core.math.vector;

/**
 * Created on 9/13/2016.
 */

public class Vector2D extends Vector
{
	public Vector2D()
	{
		super(2);
	}

	public Vector2D(double x, double y)
	{
		super(new double[]
		{
				x, y
		});
	}

	@Override
	public Vector2D sub(Vector b)
	{
		return (Vector2D)super.sub(b);
	}

	public double getX()
	{
		return getValue(0);
	}

	public double getY()
	{
		return getValue(1);
	}
}
